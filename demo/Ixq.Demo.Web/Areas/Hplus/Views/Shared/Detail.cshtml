@using Ixq.Core.Entity
@using Ixq.UI
@model IPageEditViewModel
@{
    Layout = "~/Areas/Hplus/Views/Shared/_LayoutHplusIframe.cshtml";
    var entityName = Model.EntityDto.GetType().Name;
}
@section styles{
    @Html.RenderStyles()
}

<div class="wrapper wrapper-content animated fadeIn">
    <div class="row">
        <div class="col-sm-12">
            <div class="ibox ">
                <div class="ibox-title">
                    <h5>详情-静态页面</h5>
                </div>
                <div class="ibox-content">
                    <form class="form-horizontal">
                        @foreach (IEntityPropertyMetadata item in Model.PropertyMenberInfo)
                        {
                            @Html.PropertyViewer(item,Model.EntityDto)
                        }
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts{
    @Html.RenderScripts()
}